{
  "linters": {
    "enable-all": true,
    "disable": [
      "lll",
      "gochecknoglobals",
      "bodyclose",
      "wsl",
      "funlen",
      "maligned",
      "exhaustivestruct",
      "gci",
      "wrapcheck",
      "varnamelen",
      "testpackage",
      "exhaustive",
      "gomnd",
      "thelper",
      "paralleltest"
    ]
  },
  "linters-settings": {
    "goimports": {
      "local-prefixes": "github.com/hedhyw/gherkingen/"
    }
  }
}
